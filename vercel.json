{
  "rewrites": [
    { "source": "/api/:path*", "destination": "/src/pages/api/:path*" }
  ],
  "functions": {
    "src/pages/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/[^.]+", "dest": "/", "status": 200 }
  ]
}